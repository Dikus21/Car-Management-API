  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "vercel-build": "echo 'vercel-build'",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "dev": "nodemon ./src/server.ts",
    "staging": "node ./dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "initiateMigration": "yarn run typeorm migration:generate -d src/config/Database.ts src/migrations/InitialMigration",
    "migrate": "yarn run typeorm migration:run -d src/config/Database.ts",
    "revertMigration": "yarn run typeorm migration:revert -d src/config/Database.ts",
    "seeder": "ts-node ./src/seeders/RunSeeders.ts",
    "drop": "yarn run typeorm schema:drop -d src/config/Database.ts"
  },
